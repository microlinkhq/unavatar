    "cors": "~2.8.5",
    "data-uri-regex": "~0.1.4",
    "data-uri-to-buffer": "~3.0.1",
    "debug-logfmt": "~1.0.4",
    "got": "~11.8.5",
    "helmet": "~5.1.0",
    "https-tls": "~1.0.12",
    "is-absolute-url": "~3.0.3",
    "is-email-like": "~1.0.0",
    "is-url-http": "~2.3.2",
    "lodash": "~4.17.21",
    "memoize-one": "~6.0.0",
    "morgan": "~1.10.0",
    "p-any": "~3.0.0",
    "p-reflect": "~2.1.0",
    "p-timeout": "~4.1.0",
    "polka": "~0.5.2",
    "qsm": "~2.1.2",
    "reachable-url": "~1.6.11",
    "serve-static": "~1.15.0",
    "top-user-agents": "~1.0.45",
    "unique-random-array": "~2.0.0",
    "url-regex": "~5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "browser-sync": "latest",
    "concurrently": "latest",
    "conventional-github-releaser": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "gulp": "latest",
    "gulp-autoprefixer": "latest",
    "gulp-concat": "latest",
    "gulp-cssnano": "latest",
    "gulp-sass": "latest",
    "gulp-strip-css-comments": "latest",
    "gulp-uglify": "latest",
    "hack": "latest",
    "nano-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "prismjs": "latest",
    "sass": "latest",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest",
    "untracked": "latest"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "bin",
    "gulpfile.js",
    "public",
    "src"
  ],
  "scripts": {
    "build": "gulp build && untracked > .nowignore",
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "deploy": "now -e NODE_ENV=production --public && now alias && now rm unavatar --safe --yes",
    "dev": "TZ=UTC NODE_ENV=development DEBUG=\"unavatar*\" npx vercel dev",
    "dev:docs": "concurrently \"npm run dev:docs:server\" \"npm run dev:docs:src\"",
    "dev:docs:server": "cd public && browser-sync start --server --files \"index.html, **/*.*\"",
    "dev:docs:src": "gulp",
    "lint": "standard-markdown && standard",
    "postrelease": "npm run release:tags && npm run release:github",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin}/**/*.js --single-quote",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "start": "TZ=UTC NODE_ENV=production DEBUG=\"unavatar*\" node src/server.js",
    "test": "exit 0",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "private": true,
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nano-staged": {
    "*.js,!*.min.js,": [
      "prettier-standard"
    ],
    "*.md": [
      "standard-markdown"
    ],
    "package.json": [
      "finepack"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx nano-staged"
  }
}
